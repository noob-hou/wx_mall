.swiper image {
  width: 100%;
}
.nav {
  display: flex;
}
.nav navigator {
  flex: 1;
  padding: 20rpx;
}
.nav navigator image {
  width: 100%;
}
.floor-item .title {
  padding: 10rpx 0;
}
.floor-item .title image {
  width: 100%;
}
.floor-item .product_list {
  overflow: hidden;
}
.floor-item .product_list navigator {
  float: left;
  width: 33.33%;
}
.floor-item .product_list navigator:nth-last-child(-n+4) {
  height: 12865.38vw/232/2;
}
.floor-item .product_list navigator:nth-last-child(-n+4) image {
  height: calc(33.33vw*386/232/2);
  border-left: 10rpx solid #fff;
}
.floor-item .product_list navigator image {
  width: 100%;
  height: 100%;
}
